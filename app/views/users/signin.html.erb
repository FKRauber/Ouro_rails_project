<%= form_for @user, url: "/users/login" do |f| %>
	<div class="field">
		<%= f.label :name, "Select your name" %><br><%= f.collection_select(:id, @users, :id, :name) %>
	</div>
	<div class="actions"><%= f.submit "Sign In" %></div><br>


	<div class="actions"><%= link_to "Sign in with Github", "/auth/github" %></div>

<% end %>